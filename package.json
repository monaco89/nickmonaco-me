{
  "name": "nick-monaco",
  "private": true,
  "description": "Nick Monaco's blog, projects, hopes and dreams",
  "version": "0.1.0",
  "author": "Nick Monaco",
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@testing-library/cypress": "^7.0.0",
    "eslint-plugin-cypress": "^2.11.1",
    "gatsby": "^2.20.12",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-canonical-urls": "^2.3.1",
    "gatsby-plugin-csp": "^1.1.3",
    "gatsby-plugin-emotion": "^4.2.1",
    "gatsby-plugin-google-analytics": "^2.3.1",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-offline": "^3.1.2",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-s3": "^0.3.3",
    "gatsby-plugin-sharp": "^2.5.4",
    "gatsby-remark-prismjs": "^3.4.1",
    "gatsby-remark-reading-time": "^1.1.0",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-remark": "^2.7.1",
    "gatsby-transformer-sharp": "^2.4.4",
    "prism-themes": "^1.4.0",
    "prismjs": "^1.20.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-feather": "^2.0.8",
    "react-helmet": "^5.2.1",
    "react-player": "^2.6.2",
    "typeface-nunito": "1.1.3"
  },
  "devDependencies": {
    "cypress": "^5.1.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.6",
    "prettier": "^2.0.3",
    "prettier-eslint-cli": "^5.0.0",
    "start-server-and-test": "^1.11.3"
  },
  "keywords": [
    "gatsby"
  ],
  "lint-staged": {
    "**/*.js": [
      "eslint --fix-dry-run",
      "prettier-eslint --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "lint": "eslint 'src/**/*.{js,jsx}' --fix-dry-run",
    "format": "prettier --write 'src/**/*.{js,jsx,css}'",
    "deploy": "gatsby-plugin-s3 deploy --yes && aws cloudfront create-invalidation --distribution-id $CLOUDFRONT_DISTRIBUTION --paths \"/*\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/monaco89/nick-monaco-me"
  }
}
