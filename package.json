{
  "name": "nick-monaco",
  "private": false,
  "description": "Nick Monaco's blog, projects, hopes and dreams",
  "version": "1.0.0",
  "author": "Nick Monaco",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.1.2",
    "@emotion/styled": "^11.1.5",
    "@testing-library/cypress": "^8.0.2",
    "eslint-plugin-cypress": "^2.11.1",
    "gatsby": "^4.14.0",
    "gatsby-plugin-canonical-urls": "^4.14.0",
    "gatsby-plugin-csp": "^1.1.3",
    "gatsby-plugin-emotion": "^7.14.0",
    "gatsby-plugin-google-analytics": "^4.14.0",
    "gatsby-plugin-image": "^2.15.0",
    "gatsby-plugin-manifest": "^4.14.0",
    "gatsby-plugin-offline": "^5.14.0",
    "gatsby-plugin-react-helmet": "^5.14.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-robots-txt": "^1.5.3",
    "gatsby-plugin-s3": "^0.3.3",
    "gatsby-plugin-sharp": "^4.14.0",
    "gatsby-plugin-sitemap": "^5.14.0",
    "gatsby-remark-images": "^6.14.0",
    "gatsby-remark-prismjs": "^6.14.0",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-source-filesystem": "^4.14.0",
    "gatsby-transformer-remark": "^5.14.0",
    "gatsby-transformer-sharp": "^4.14.0",
    "google-map-react": "^2.1.9",
    "prism-themes": "^1.4.0",
    "prismjs": "^1.20.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-feather": "^2.0.9",
    "react-helmet": "^6.1.0",
    "react-player": "^2.7.0"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "^11.2.0",
    "cypress": "^9.6.1",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "prettier": "^2.2.1",
    "start-server-and-test": "^1.11.6"
  },
  "keywords": [
    "gatsby"
  ],
  "lint-staged": {
    "**/*.js": [
      "eslint --fix-dry-run",
      "prettier --write"
    ]
  },
  "scripts": {
    "build": "GATSBY_ENV=production gatsby build",
    "develop": "set GATSBY_ENV=development && gatsby develop",
    "lint": "eslint 'src/**/*.{js,jsx}' --fix-dry-run",
    "format": "prettier --write 'src/**/*.{js,jsx,css}'",
    "deploy": "gatsby-plugin-s3 deploy --yes && aws cloudfront create-invalidation --distribution-id $CLOUDFRONT_DISTRIBUTION --paths \"/*\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/monaco89/nick-monaco-me"
  }
}
